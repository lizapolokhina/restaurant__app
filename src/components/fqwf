export function App() {
  const [menuList, setMenuList] = useState(true);

  return (
    <>
    <div className="App">
      <button
      className='menuButton'
      onClick={() => setMenuList(!menuList)}
      >
        menu
      </button>
      { menuList && <MenuItem />}
    </div>
    <CartProvider>
      <Home />
      <Cart />
    </CartProvider>
    </>
  );
}